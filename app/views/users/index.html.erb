        <div class="index_central_posts_container grey">

            <div class="new_post_form_container homepage">
                <div class="post_edit_frame">
                    <%# <%=turbo_frame_tag @post do %> 
                    <div class="new_post_search">
                    <%= link_to "What's on your mind, #{@user_first_name}?", new_post_path %>
                    </div>
                    <%# <%end%> 
                </div>
            </div>

            <div class="index_posts">
                    <%@timeline_posts.each do |post|%>
                    <%=turbo_frame_tag post do %>
                    <div class="index_post_container">
                    <div class="index_post_name">
                    <%= post.user.full_name%>
                    </div>
                    <div class="index_post_info">
                    <%= post.information%>
                    </div>
                    <div class="index_post_edit">
                    <%if post.user.id == current_user.id%>
                    <%=link_to "Edit Post", edit_post_path(post)%>    
                    <%end%>
                    </div>
                    </div>
                    <%end%>
            </div>
            <%end%>

        </div>

        <div class="index_right_friends_bar grey">

            <div class="index_friend_requests">
                Friend Requests:
                <%if current_user.pending_friend_requests.empty?%>
                No new friend Requests
                <%else%>
                    <%current_user.pending_friend_requests.each do |friend_request|%>
                    <%=friend_request.sent_from.full_name%>
                    
                    <%=form_with model:@friendship, data:{turbo:false} do |f|%>
                    <%=f.hidden_field :friend_one_id, value: friend_request.sent_from.id %> 
                    <%=f.hidden_field :friend_two_id, value:current_user.id%> 
                    <%=f.submit "Accept Friend Request"%> 
                    <%end%>

                    <%end%>
                <%end%>

            </div>

            <div class="index_friends">

                <div class="index_pending_friends">
                    Pending friends:
                    <%if current_user.fr_sent.empty?%>
                    No friend requests awaiting response 

                    <%else%>
                        <%current_user.fr_sent.each do |fr|%>
                        <div class="index_pending_request">
                            <div class="index_pending_request_logo">LOGO</div>
                            <div class="index_pending_request_name"><%=fr.sent_to.full_name%></div>
                        </div>
                        <%end%>
                    <%end%>
                </div>

                <div class="index_friends_list">
                Friends:
                <%current_user.friends_as_one.each do |friend|%>
                <div class="index_current_friends_container">
                <div class="index_current_friends_logo">LOGO</div>
                <div class="index_current_friends_name"><%=friend.full_name%> </div>
                <%end%>
                </div>
            </div>
